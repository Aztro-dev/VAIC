.picker{
  background-color: #FFF;
  margin-left: 80%;
  display: grid;
  flex-direction: column;
  height: 80%;
  margin-top: 5%;
  margin-bottom: 5%;
}

.title{
  font-size: 50%;
}

.button{
  height: 5%;
  background-color: #333333;
  color: #FFFFFF;
  border-width: 2px 2px 2px 2px;
}

.list{
  justify-content: center;
  align-items: center;
  display: grid;
  margin: auto;
  width: 90%;
}
